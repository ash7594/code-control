<html>
<head>
<%- include req %>
<title> Leaderboard </title>
</head>
<body>
<%- include header %>
<table class = "table table-striped table-bordered table-condensed">
    <thead>
        <th> Position </th>
        <th> Name </th>
        <th> Experience </th>
    </thead>
    <% topChars.forEach(function(char, i) { %>
        <tr>
            <td> <%= i + 1 %> </td>
            <td> <%- link_to(char.name, '/c/' + char.name) %> </td>
            <td> <%= char.experience %> </td>
        </tr>
    <% }); %>
</table>
<%- link_to('More', '/leaderboard?from=' + (from + 100)) %>
</body>
</html>
